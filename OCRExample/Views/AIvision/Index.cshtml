
<div class="meatsure-box">
    <div class="measurement-form">
        <div class="row">
            <div class="col-lg-12">
                <div class="ttl-style-2">
                    <h2>AddMeasurement</h2>
                </div>
            </div>
        </div>
        <div>
            <div class="row">

                <!--date-->
                <div class="col-lg-6 col-md-6 col-form-1">
                    <div class="form-group">
                        <label for="date" class="form-label"> Date:</label>
                        <input type="text" class="form-control datepicker" id="add_date_scan" placeholder="yyyy/mm/dd" name="date" autocomplete="off" required
                               oninput="setCustomValidity('');" />
                    </div>
                </div>
                <!--time-->
                <div class="col-lg-6 col-md-6 col-form-1">
                    <div class="form-group">
                        <label for="time" class="form-label">Time:</label>
                        <div class="time-input-box">
                            <div class="time-input">
                                <div class="select-box">
                                    <select name="hour" id="hour_scan" class="form-control" required>
                                        <option value="1">01</option>
                                        <option value="2">02</option>
                                        <option value="3">03</option>
                                        <option value="4">04</option>
                                        <option value="5">05</option>
                                        <option value="6">06</option>
                                        <option value="10">10</option>
                                    </select>
                                </div>
                            </div>
                            <span>:</span>
                            <div class="time-input">
                                <div class="select-box">
                                    <select name="minute" id="minute_scan" class="form-control" required>
                                        <option value="12">12</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--scan-->
                <div class="col-lg-6 col-md-6 col-form-1">
                    <video id="videoElement" class="videoElement" autoplay muted playsinline hidden></video>
                    <div class="add-scanning" hidden id="scanning_view"></div>
                </div>
                <div class="col-lg-12 col-form-1 mb-0">
                    <button class="btn-style-2" id="Scan">Scan 1.1</button>
                    <button class="btn-style-2" id="Rescan1" hidden>Rescan</button>
                    <button class="btn-style-2" id="custom_Scan">Scan 1.2</button>
                    <button class="btn-style-2" id="Rescan2" hidden>Rescan</button>
                </div>
                <div class="col-lg-12 col-form-1 mb-0">
                    <button class="btn-style-2" id="tensorflow">tensorflow test</button>
                </div>
                <div id="topcontent"></div>
                <div id="info"></div>
                <div class="col-lg-4 col-md-6 col-form-1">
                    <div id="content"></div>
                </div>
                <!--Systolic-->
                <div class="col-lg-4 col-md-6 col-form-1">
                    <div class="form-group">
                        <label for="sys" class="form-label">Systolic:</label>
                        <div class="weight-input-box">
                            <div class="weight-input">
                                <div class="select-box">
                                    <input id="sys" class="form-control" required />
                                </div>
                            </div><span class="unit">mmHg</span>
                        </div>
                    </div>
                </div>
                <!--Diastolic-->
                <div class="col-lg-4 col-md-6 col-form-1">
                    <div class="form-group">
                        <label for="dia" class="form-label">Diastolic:</label>
                        <div class="weight-input-box">
                            <div class="weight-input">
                                <div class="select-box">
                                    <input id="dia" class="form-control" required />
                                </div>
                            </div><span class="unit">mmHg</span>
                        </div>
                    </div>
                </div>
                <!--Pulse-->
                <div class="col-lg-4 col-md-6 col-form-1">
                    <div class="form-group">
                        <label for="pul" class="form-label">Pulse:</label>
                        <div class="weight-input-box">
                            <div class="weight-input">
                                <div class="select-box">
                                    <input id="pul" class="form-control" required />
                                </div>
                            </div><span>bpm</span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12 col-form-1 mb-0">
                    <div class="submit-box">
                        <button class="btn-style-2" id="scan_save_button">
                            <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true" hidden id="lodding_spinner"></span>
                            <span>Save</span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.AntiForgeryToken()
</div>
<script src="https://cdn.jsdelivr.net/npm/@@tensorflow/tfjs@3.12.0/dist/tf.min.js"></script>
<script src="https://unpkg.com/@@microsoft/customvision-tfjs@1.3.0"></script>
<script src="~/lib/moment/moment.js"></script>
<script src="~/js/aiversion.js?20250108"></script>
<script src="~/js/customai.js?20250108"></script>
